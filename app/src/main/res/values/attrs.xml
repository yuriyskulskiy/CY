<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="Labeled">
        <attr name="nst_label_text" format="string" />
    </declare-styleable>

    <declare-styleable name="LabeledTextView" parent="Labeled">
        <attr name="nst_text" format="string"/>
    </declare-styleable>

    <declare-styleable name="MaterialSpinner">
        <attr name="ms_baseColor" format="integer"/>
        <attr name="ms_lineColor" format="integer"/>
        <attr name="ms_highlightColor" format="integer"/>
        <attr name="ms_colorError" format="integer"/>
        <attr name="ms_error" format="string"/>
        <attr name="ms_hint" format="string"/>
        <attr name="ms_floatingLabelTextSize" format="dimension"/>
        <attr name="ms_floatingLabelText" format="string"/>
        <attr name="ms_floatingLabelColor" format="color"/>
        <attr name="ms_multiline" format="boolean"/>
        <attr name="ms_nbErrorLines" format="integer"/>
        <attr name="ms_typeface" format="string"/>
        <attr name="ms_alignLabels" format="boolean"/>
        <attr name="ms_thickness" format="dimension"/>
        <attr name="ms_thickness_error" format="dimension"/>
        <attr name="ms_arrowSize" format="dimension"/>
        <attr name="ms_arrowColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="ChisyRelativeLayout">
        <attr name="value" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="SlidingFrames">
        <attr name="nst_tail_size" format="dimension" />
    </declare-styleable>

    <declare-styleable name="IconTextButton">
        <attr name="nst_itb_text" format="string"/>
        <attr name="nst_itb_icon" format="reference"/>
    </declare-styleable>

    <declare-styleable name="RobotoFontTextView">
        <attr name="tv_robotoFont" format="string"/>
    </declare-styleable>

    <declare-styleable name="RobotoFontButton">
        <attr name="btn_robotoFont" format="string"/>
    </declare-styleable>

    <declare-styleable name="RobotoFontTextInputLayout">
        <attr name="til_robotoFont" format="string"/>
    </declare-styleable>

    <declare-styleable name="RobotoFontEditText">
        <attr name="et_robotoFont" format="string"/>
    </declare-styleable>

</resources>